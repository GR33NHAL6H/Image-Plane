 addUserKnob {41 samples l "Scanline Samples" T ScanlineRender1.samples}
 addUserKnob {26 ""}
 addUserKnob {26 txt2 l "" -STARTLINE T "<div style=\"text-align: left; font-size: 10px; color: #666666; font-style: italic;\">\n  v 1.1 : 2024-04\n</div>\n\n"}
}
